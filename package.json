{
	"name": "@heiwa4126/fizzbuzz",
	"version": "0.0.0",
	"description": "NPM package tutorial",
	"type": "commonjs",
	"main": "src/index.js",
	"bin": {
		"heiwa4126-fizzbuzz": "./src/cli.js"
	},
	"engines": {
		"node": ">=16"
	},
	"scripts": {
		"start": "node src/cli.js",
		"test": "tape __tests__/**/*.test.js",
		"lint": "biome lint ./src ./__tests__",
		"fix": "biome check --apply ./src ./__tests__",
		"lint:package": "npmPkgJsonLint ./package.json"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/heiwa4126/npm-fizzbuzz.git"
	},
	"keywords": ["demo", "tutorial", "template"],
	"author": {
		"name": "heiwa4126",
		"email": "heiwa4126@gmail.com"
	},
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/heiwa4126/npm-fizzbuzz/issues"
	},
	"homepage": "https://github.com/heiwa4126/npm-fizzbuzz#readme",
	"files": ["package.json", "README.md", "LICENSE", "src/"],
	"ignore": ["**/*.test.js/", "**/.DS_Store", "temp/"],
	"devDependencies": {
		"@biomejs/biome": "^1.6.4",
		"npm-package-json-lint": "~7",
		"tape": "~5"
	}
}
